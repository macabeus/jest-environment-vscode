{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "isolatedModules": true,
    "importsNotUsedAsValues": "error",
    "strict": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./build",
  },
}
